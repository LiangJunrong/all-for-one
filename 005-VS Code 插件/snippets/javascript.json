{
  "@fnComments": {
		"prefix": "@fnComments",
		"body": [
			"/**",
			"* @name ${1:函数名称}",
      "* @description ${2:函数名字}",
      "* @param { ${3|Object,Array,Boolean,Number,String|} } ${4:参数名称}",
			"* @return ${5:接口返回}",
			"*/\n"
		],
		"description": "函数注释"
	},
	"js": {
		"prefix": "js",
		"body": [
      "import React from 'react';",
      "",
      "function $TM_FILENAME_BASE() {",
      "  return (",
      "    <div>",
      "      $1",
      "    </div>",
      "  )",
      "}",
      "",
      "export default $TM_FILENAME_BASE;"
		],
		"description": "React 无状态组件"
  }
}