{
	"name": "regex-study-and-practice",
	"displayName": "regex-study-and-practice",
	"publisher": "jsliang",
	"description": "jsliang 制作的关于正则表达式的学习和练习的插件",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.42.0"
	},
	"keywords": [
		"jsliang",
		"javascript",
		"regex",
		"regex study",
		"regex practice"
	],
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/LiangJunrong/all-for-one"
	},
	"icon": "images/icon.jpg",
	"main": "./out/extension.js",
	"activationEvents": [
		"onView:study",
		"onView:practice"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "regex",
					"title": "正则表达式学习和练习",
					"icon": "images/regex.svg"
				}
			]
		},
		"views": {
			"regex": [
				{
					"id": "study",
					"name": "正则表达式"
				},
				{
					"id": "practice",
					"name": "npm 包"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "1.42.0",
		"typescript": "^3.9.4",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"jsonc-parser": "^0.4.2",
		"marked": "^1.1.0",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	}
}
