{
	"name": "regex-study-and-practice",
	"displayName": "regex-study-and-practice",
	"publisher": "jsliang",
	"description": "jsliang 制作的关于正则表达式的学习和练习的插件",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"keywords": [
		"jsliang",
		"javascript",
		"regex",
		"regex study",
		"regex practice"
	],
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/LiangJunrong/all-for-one"
	},
	"activationEvents": [
		"onView:regex"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "regex",
					"title": "正则表达式学习和训练",
					"icon": "images/regex.svg"
				}
			]
		},
		"views": {
			"regex": [
				{ "id": "study", "name": "学习" },
				{ "id": "practice", "name": "练习" }
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
